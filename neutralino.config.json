{
  "$schema": "https://raw.githubusercontent.com/neutralinojs/neutralinojs/main/schemas/config-schema.json",
  "applicationName": "bolt.gives",
  "version": "1.0.2",
  "framework": "neutralino",
  "defaultMode": "window",
  "window": {
    "title": "bolt.gives",
    "width": 1280,
    "height": 800,
    "minWidth": 800,
    "minHeight": 600,
    "resizable": true,
    "fullscreen": false,
    "center": true
  },
  "modes": {
    "window": {
      "title": "bolt.gives",
      "width": 1280,
      "height": 800,
      "minWidth": 800,
      "minHeight": 600,
      "resizable": true,
      "fullscreen": false,
      "center": true
    }
  },
  "cli": {
    "binaryName": "bolt-gives",
    "resourcesPath": "build/client",
    "port": 5173,
    "frontendLibrary": {
      "name": "react",
      "端口": 5173
    }
  },
  "security": {
    "enableCSP": true,
    "csp": "default-src 'self'; script-src 'self' 'unsafe-inline' 'unsafe-eval' blob:; style-src 'self' 'unsafe-inline'; connect-src 'self' http://localhost:* https://*; img-src 'self' data: blob: https:; font-src 'self' data:; frame-src 'self' blob:;"
  },
  "libraryVersion": "5.0.0"
}
